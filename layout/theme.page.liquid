<!doctype html>
<html class="no-js" data-wf-page="{% if template contains 'manifesto'%}61ee17d670654180df4503e6{% else %}61e7aa41824d2f97955c1e9c{% endif %}" data-wf-site="61e7aa41824d2f42455c1e9b" lang="{{ request.locale.iso_code }}">
